Holiday VI - Shark Pontoon.
https://www.codewars.com/users/Kseniia%20Sydorenko/completed.
function shark(pontoonDistance, sharkDistance, youSpeed, sharkSpeed, dolphin){
 dolphin?(sharkSpeed = sharkSpeed/2):sharkSpeed;
 if (sharkDistance === 0){
 return 'Shark Bait!'}
 else {
   let timeToGetEaten = sharkDistance/sharkSpeed;
   let timeToGetToPontoon = pontoonDistance/youSpeed;
   if (timeToGetToPontoon >= timeToGetEaten){
    return 'Shark Bait!'}
   else {
   return 'Alive!'}
 }
}
