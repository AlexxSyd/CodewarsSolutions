Simple calculator

function calculator(a,b,sign){
if (typeof(a)!=='number' || typeof(b)!=='number') return 'unknown value';
else {
switch (sign){
case '+':return  a+b;
break;
case '-':return a-b;
break;
case '/':return  a/b;
break;
case '*':return a*b;
break;
default:return 'unknown value';
}}}
